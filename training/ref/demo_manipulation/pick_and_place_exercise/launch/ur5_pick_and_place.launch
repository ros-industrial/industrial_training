<?xml version="1.0"?>
<launch>

  <!-- pick and place node -->
  <node pkg="pick_and_place_exercise" type="pick_and_place_node" name="pick_and_place_node" output="screen">
    <rosparam command="load" file="$(find pick_and_place_exercise)/config/ur5/pick_and_place_parameters.yaml"/>
  </node>

  <include file="$(find ur5_environment_moveit_config)/launch/move_group.launch">
    <arg name="publish_monitored_planning_scene" value="true" />
  </include>

  <rosparam command="load" file="$(find ur5_environment_moveit_config)/config/joint_names.yaml"/>

  <include file="$(find ur5_environment_moveit_config)/launch/default_warehouse_db.launch" />

</launch>
