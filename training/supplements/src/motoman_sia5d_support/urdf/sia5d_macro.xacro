<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">

	<property name="pi" value="3.141592654" />
	<property name="deg" value="0.017453293" /> <!--degrees to radians-->
	
	<xacro:macro name="motoman_sia5d" params="prefix">

  <link name="${prefix}base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/visual/base.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/collision/base.dae" />
      </geometry>
    </collision>
  </link>
  
  <link name="${prefix}link_s">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/visual/link_s.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/collision/link_s.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_s" type="revolute">
    <origin xyz="0 0 0.31" rpy="0 0 0" /> <!--r=${90*deg}-->
    <parent link="${prefix}base_link" />
    <child link="${prefix}link_s" />
    <axis xyz="0 0 1" />
    <limit lower="${-180*deg}" upper="${180*deg}" effort="0" velocity="${200*deg}" />
  </joint>

  <link name="${prefix}link_l">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/visual/link_l.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/collision/link_l.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_l" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="${prefix}link_s" />
    <child link="${prefix}link_l" />
    <axis xyz="0 1 0" />
    <limit lower="${-110*deg}" upper="${110*deg}" effort="0" velocity="${200*deg}" />
  </joint>

  <link name="${prefix}link_e">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/visual/link_e.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/collision/link_e.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_e" type="revolute">
    <origin xyz="0 0 0.27" rpy="0 0 0" />
    <parent link="${prefix}link_l" />
    <child link="${prefix}link_e" />
    <axis xyz="0 0 1" />
    <limit lower="${-170*deg}" upper="${170*deg}" effort="0" velocity="${200*deg}" />
  </joint>
  
  <link name="${prefix}link_u">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/visual/link_u.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/collision/link_u.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_u" type="revolute">
    <origin xyz="0.085 0 0" rpy="0 0 0" />
    <parent link="${prefix}link_e" />
    <child link="${prefix}link_u" />
    <axis xyz="0 1 0" />
    <limit lower="${-90*deg}" upper="${115*deg}" effort="0"  velocity="${200*deg}" />
  </joint>

  <link name="${prefix}link_r">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/visual/link_r.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/collision/link_r.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_r" type="revolute">
    <origin xyz="0.27 0 0.06" rpy="0 0 0" />
    <parent link="${prefix}link_u" />
    <child link="${prefix}link_r" />
    <axis xyz="-1 0 0" />
    <limit lower="${-90*deg}" upper="${90*deg}" effort="0" velocity="${200*deg}" />
  </joint>

  <link name="${prefix}link_b">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/visual/link_b.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/collision/link_b.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_b" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="${prefix}link_r" />
    <child link="${prefix}link_b" />
    <axis xyz="0 1 0" />
    <limit lower="${-110*deg}" upper="${110*deg}" effort="0" velocity="${230*deg}" />
  </joint>

  <link name="${prefix}link_t">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/visual/link_t.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_config/meshes/sia5/collision/link_t.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_t" type="revolute">
    <origin xyz="0.134 0 0" rpy="0 0 0" />
    <parent link="${prefix}link_b" />
    <child link="${prefix}link_t" />
    <axis xyz="-1 0 0" />
    <limit lower="${-90*deg}" upper="${90*deg}" effort="0" velocity="${350*deg}" />
  </joint>
  
  <link name="${prefix}tool0" />
  
  <joint name="${prefix}link_t-tool0" type="fixed" >
    <origin xyz="0.011 0 0" rpy="${-90*deg} ${-90*deg} ${-90*deg}"/>
    <parent link="${prefix}link_t" />
    <child link="${prefix}tool0" />
  </joint>
  
	</xacro:macro>
</robot>

