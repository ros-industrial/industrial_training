<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
	<xacro:macro name="irb_5400" params="prefix" >
		<!-- link list -->
    <link name="${prefix}base_link">
      <visual>
        <geometry>
          <mesh filename="package://abb_common/meshes/irb_5400/visual/BASE_LINK.STL" />
